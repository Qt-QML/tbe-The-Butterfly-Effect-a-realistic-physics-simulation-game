<!DOCTYPE mydocument>
<tbe-level>
    <levelinfo>
        <title>Geyser</title>
        <title lang="nl" >Geiser</title>
        <author>idea: Peter van Ginneken, implementation: Klaas van Gend</author>
        <license>GPLv2</license>
        <description>A volley ball must topple a pin from the other end.</description>
        <description lang="nl" >Een volleybal hoort de kegel de andere kant op te kegelen.</description>
        <date>4/17/10</date>
    </levelinfo>
    <toolbox>
        <toolboxitem count="1" name="Ramp /" >
            <name lang="nl" >Helling /</name>
            <object width="1.6" height="0.3" type="LeftRamp" >
                <property key="ImageName" >LeftRamp</property>
            </object>
        </toolboxitem>
        <toolboxitem count="1" name="Ramp \" >
            <name lang="nl" >Helling \</name>
            <object width="1.4" height="0.5" type="RightRamp" >
                <property key="ImageName" >RightRamp</property>
            </object>
        </toolboxitem>
        <toolboxitem count="1" name="Beam" >
            <name lang="nl" >Balk</name>
            <object width="1.2" height="0.1" type="RectObject" >
                <property key="Bounciness" >0.0</property>
                <property key="Friction" >0.5</property>
                <property key="ImageName" >i-beam</property>
                <property key="Resizable" >none</property>
                <property key="Mass" >500.0</property>
                <property key="ZValue" >3.0</property>
            </object>
        </toolboxitem>
    </toolbox>
