<!DOCTYPE mydocument>
<tbe-level>
    <levelinfo>
        <title>Coconut instead of Cola</title>
        <author>Klaas van Gend</author>
        <license>GPLv2</license>
        <description>Shoot a coconut from the palm.</description>
        <date>10/11/09</date>
    </levelinfo>
    <toolbox>
        <toolboxitem count="2" name="Cola Crate">
            <object width="0.85" height="0.6" type="RectObject">
                <tooltip>This is how you properly buy cola:&lt;br>a dozen bottles of that precious brown liquid at a time!</tooltip>
                <property key="ImageName">cola-crate</property>
                <property key="Mass">18</property>
                <property key="Friction">1</property>
                <property key="Resizable">none</property>
                <property key="Rotatable">true</property>
                <property key="Bounciness">0.1</property>
            </object>
        </toolboxitem>
        <toolboxitem count="1" name="Wooden bar">
            <object width="1.5" X="1.2" Y="0.8" height="0.2" type="BirchBar">
                <property key="Friction">0</property>
                <property key="Resizable">none</property>
                <property key="Bounciness">0.0</property>
            </object>
        </toolboxitem>
    </toolbox>
    <scene>
        <scenesize width="6.0" height="4"/>
        <predefined>
            <object width="7.000" X="5.000" Y="3.50" height="2.500" type="Scenery" angle="0.000">
                <property key="ImageName">the-sun</property>
            </object>
            <object width="2.8" X="4.4" Y="2.1" height="4.2" type="Scenery">
                <property key="ImageName">palmtree</property>
            </object>
            <object width="6" X="3" Y="0.05" type="Floor">
                <property key="ImageName">Yellow-to-brown</property>
                <property key="Friction">0.6</property>
                <property key="Bounciness">0.1</property>
            </object>
            <object width="0.2" X="0.4" Y="0.25" height="0.3" type="RightRamp" angle="0">
                <property key="ImageName">Empty</property>
            </object>
            <object X="0.75" Y="1.37" type="ColaMintBottle" angle="3.14">
                <property key="Thrust">5</property>
                <property key="Mass">1</property>
            </object>
            <object width="0.1" X="4.4" Y="2.69" height="0.1" type="RectObject">
                <property key="ImageName">Empty</property>
            </object>
            <object X="4.4" Y="2.85" type="BowlingBall" ID="Coconut">
                <property key="ImageName">coconut</property>
                <tooltip>Hey, I'm a coconut!</tooltip>
            </object>
            <object width="0.1" X="1.175" Y="0.15" height="0.1" type="RectObject" angle="0">
                <property key="ImageName">boulder</property>
                <property key="Bounciness">0.1</property>
            </object>
            <object width="0.5" X="5.75" Y="1.5" height="2.8" type="Wall"/>
            <object width="0.34" X="0.17" Y="1.05" height="1.9" type="RectObject">
                <property key="ImageName">Empty</property>
            </object>
            <object width="0.45" X="0.225" Y="1.05" height="1.9" type="Scenery">
                <property key="ImageName">006-stick-figure</property>
            </object>
            <object width="0.35" X="0.62" Y="1.775" height="0.45" type="Scenery">
                <property key="ImageName">006-stick-figure-arm</property>
            </object>
            <object width="0.1" X="5.1" Y="0.15" height="0.1" type="RectObject" angle="0">
                <property key="ImageName">boulder</property>
                <property key="Bounciness">0.1</property>
            </object>
            <object width="0.1" X="2.65" Y="0.15" height="0.1" type="RectObject" angle="0">
                <property key="ImageName">boulder</property>
                <property key="Bounciness">0.1</property>
            </object>
            <object X="2.8" Y="1.4" type="PostIt">
                <property key="page1">This is Ruud.&lt;br>&lt;br>He’s on a small desert island.</property>
                <property key="page2">A few crates of cola washed up shore lately—so he’s not thirsty.</property>
                <property key="page3">But do you see those coconuts?</property>
                <property key="page4">&lt;br>&lt;br>… a hungry Post-It Boy</property>
            </object>
        </predefined>
    </scene>
    <goals>
        <goal type="positionchange">
            <property key="ybelow">1.0</property>
            <property key="object">Coconut</property>
        </goal>
    </goals>
    <hints>
        <hint X="1.66" Y="0.41" number="1" object="RectObject"/>
        <hint X="1.0" Y="0.9" number="2" object="BirchBar"/>
    </hints>
</tbe-level>
